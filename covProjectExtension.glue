# Extend COV-GLUE with betacoronavirus data 

project cov

	# Delete previous extension build
	run file glue/betacovDeleteExtensionBuild.glue

	# import cov-gisaid individual fasta
	run file glue/betacovImportSources.glue

	# import cov-gisaid individual fasta
	run file glue/betacovAlignments.glue


	# define constrained alignment tree
	run file glue/extensionAlignmentTree.glue
  
	# Validate
	validate

	exit
