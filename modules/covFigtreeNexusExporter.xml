<nexusExporter>
	<freemarkerAnnotationGenerator>
		<annotationName>source_and_seq_id</annotationName>
		<template>${renderNestedProperty('sequence.source.name')}/${renderNestedProperty('sequence.sequenceID')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>country</annotationName>
		<template>${renderNestedProperty('sequence.m49_country.display_name')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>isolate</annotationName>
		<template>${renderNestedProperty('sequence.isolate')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>place_sampled</annotationName>
		<template>${renderNestedProperty('sequence.place_sampled')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>sub_region</annotationName>
		<template>${renderNestedProperty('sequence.m49_country.m49_sub_region.id')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>region</annotationName>
		<template>${renderNestedProperty('sequence.m49_country.m49_region.id')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>orf_8_aa_84</annotationName>
		<template>${renderNestedProperty('sequence.orf_8_aa_84')}</template>
	</freemarkerAnnotationGenerator>
	<freemarkerAnnotationGenerator>
		<annotationName>pang_lineage</annotationName>
		<template>${renderNestedProperty('sequence.pang_lineage')}</template>
	</freemarkerAnnotationGenerator>
	<figtreeProperty type="integer" name="tipLabels.fontSize">13</figtreeProperty>
	<figtreeProperty name="tipLabels.colorAttribute">region</figtreeProperty>
	<figtreeProperty name="tipLabels.displayAttribute">isolate</figtreeProperty>
	<figtreeProperty type="integer" name="nodeLabels.fontSize">13</figtreeProperty>
	<figtreeProperty type="boolean" name="nodeLabels.isShown">false</figtreeProperty>
</nexusExporter>