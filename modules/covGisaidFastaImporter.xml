<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<fastaImporter>
	<sourceName>cov-gisaid</sourceName>
	<idParser>
		<matchPattern>(EPI_ISL_\d+)</matchPattern>
	</idParser>
	<!-- to handle sequence from Mexico -->
	<convertHyphensToNs>true</convertHyphensToNs>
	<!-- to handle sequence from PHE -->
	<deleteSpaces>true</deleteSpaces>
</fastaImporter>
